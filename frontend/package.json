{
    "name": "frontend",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "proto-js": "npx pbjs -t static-module -w es6 -o ./src/pkg_proto/compiled.js ../backend/proto/common.proto ../backend/proto/auth.proto ../backend/proto/matchmaking.proto ../backend/proto/game.proto",
        "proto-ts": "npx pbts -o ./src/pkg_proto/compiled.d.ts ./src/pkg_proto/compiled.js",
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs"
    },
    "devDependencies": {
        "protobufjs-cli": "^1.1.3",
        "terser": "^5.31.0",
        "typescript": "^5.6.3",
        "vite": "^5.3.1"
    },
    "dependencies": {
        "google-protobuf": "^3.21.4",
        "phaser": "^3.86.0",
        "protobufjs": "^7.4.0"
    }
}